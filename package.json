{
	"name": "dwh-audit",
	"version": "1.0.0",
	"type": "module",
	"bin": {
		"dwh-audit": "./bin/dwh-audit.js"
	},
	"main": "./index.d.ts",
	"files": [
		"bin/",
		"bigquery.js",
		"audit.js",
		"buildReport.js",
		"rebuild.js",
		"index.d.ts",
		"README.md",
		"CLAUDE.md"
	],
	"engines": {
		"node": ">=16.0.0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ak-audit/dwh-audit.git"
	},
	"bugs": {
		"url": "https://github.com/ak-audit/dwh-audit/issues"
	},
	"homepage": "https://github.com/ak-audit/dwh-audit#readme",
	"scripts": {
		"test": "vitest",
		"test:run": "vitest run",
		"test:integration": "vitest run test/integration.test.js",
		"prune": "rm -rf ./output/* && rm -rf ./dataViewer && rm -rf ./jobUser && rm -rf ./tmp",
		"serve": "npx serve ./output/reports",
		"extract": "node bigquery.js",
		"analyze": "node audit.js",
		"report": "node rebuild.js",
		"pipeline:watch": "nodemon --watch audit.js --watch buildReport.js --exec 'node audit.js && node rebuild.js'",
		"post": "npm publish --access public"
	},
	"keywords": [
		"bigquery",
		"audit",
		"data-warehouse",
		"analytics",
		"cli",
		"data-quality",
		"google-cloud",
		"schema-analysis",
		"mixpanel",
		"typescript",
		"modular"
	],
	"author": "ak@mixpanel.com",
	"license": "MIT",
	"description": "Modular CLI tool for auditing data warehouses - extract, analyze, and report on schemas, data quality, and analytics readiness",
	"dependencies": {
		"@google-cloud/bigquery": "^8.1.1",
		"google-auth-library": "^9.0.0",
		"yargs": "^18.0.0"
	},
	"devDependencies": {
		"nodemon": "^3.1.10",
		"vitest": "^3.2.4"
	}
}
